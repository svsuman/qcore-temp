{
  "name": "Playwrightscratch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:develop:headed": "TEST_ENV=develop playwright test --headed",
    "test:staging:headed": "TEST_ENV=staging playwright test --headed",
    "test:master:headed": "TEST_ENV=master playwright test --headed",
    "test:prodEU:headed": "TEST_ENV=productionEU playwright test --headed",
    "test:prodUS:headed": "TEST_ENV=productionUS playwright test --headed",
    "HealthCheckDev": "NODE_ENV=develop  npx playwright test --grep @HealthCheck ",
    "HealthCheckStg": "NODE_ENV=stage  npx playwright test --grep @HealthCheck ",
    "jenkins-config-template": "npx psl render config/$NODE_ENV.json.template --fromrole true > config/$NODE_ENV.json",
    "load-secrets-dev": "npx psl render config/develop.json.template --profile develop > config/develop.json",
    "load-secrets-stg": "npx psl render config/stage.json.template --awsprofile stage > config/stage.json",
    "test": "node scripts/runTests.js",
    "test:dev": "node scripts/runTests.js --profile dev",
    "test:stg": "node scripts/runTests.js --profile stg"
  },
